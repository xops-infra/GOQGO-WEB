# 开发环境配置
VITE_APP_TITLE=GoQGo Web Development
VITE_APP_VERSION=v0.2.0

# API配置
VITE_API_BASE_URL=http://localhost:8000
VITE_WS_BASE_URL=ws://localhost:8000

# Mock配置
# 是否启用Mock模式 (true/false)
# true: 使用模拟数据，不连接真实后端
# false: 连接真实后端API
VITE_MOCK_ENABLED=false

# Mock延迟时间（毫秒）
VITE_MOCK_DELAY=300

# 开发工具
VITE_DEV_TOOLS=true
