{
  "name": "goqgo-web",
  "version": "0.2.0",
  "description": "GoQGo AI智能体协助开发平台的Web前端界面",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prod": "vite --port 80 --host",
    "build": "npm run build:version && vite build",
    "build:version": "node scripts/build-version.js",
    "build:prod": "NODE_ENV=production npm run build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "version:check": "node scripts/build-version.js",
    "docker:build": "./docker/docker-build.sh",
    "docker:run": "./docker/docker-run.sh",
    "docker:up": "cd docker && docker-compose up -d",
    "docker:down": "cd docker && docker-compose down",
    "docker:logs": "cd docker && docker-compose logs -f",
    "deploy": "./docker/deploy.sh"
  },
  "dependencies": {
    "@types/markdown-it": "^14.1.2",
    "@vicons/ionicons5": "^0.12.0",
    "@vue/runtime-core": "^3.5.18",
    "@vueuse/core": "^13.6.0",
    "axios": "^1.6.2",
    "highlight.js": "^11.11.1",
    "markdown-it": "^14.1.0",
    "naive-ui": "^2.38.1",
    "pinia": "^2.1.7",
    "socket.io-client": "^4.7.4",
    "vue": "^3.4.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.12.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/eslint-config-typescript": "^6.0.0",
    "eslint": "^8.55.0",
    "eslint-plugin-vue": "^9.19.2",
    "prettier": "^3.1.1",
    "sass": "^1.69.5",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vue-tsc": "^1.8.25"
  }
}
